      "drawer": { // 外观渲染配置（自定义塔体视觉效果）
        "type": "DrawMulti", // 渲染类型：多重渲染（可叠加多个渲染组件）
        "drawers": [ // 渲染组件列表
            {
                "type": "DrawTurret", // 组件类型：防御塔专用渲染
                "parts": [ // 塔体渲染部件
                    {
                        "type": "HaloPart", // 部件类型：光环特效（充能时的环绕效果）
                        "radius": 5,       // 初始半径：0（未充能时不可见）
                        "radiusTo": 20,     // 充能后半径：12单位（充能时光环扩大到12）
                        "shapes": 3,                      // 形状数量：4个（生成4个三角形光环）
                        "tri": true,                       // 是否为三角形：是（光环形态为三角形）
                        "x": 0,                            // 水平偏移：0（光环横向居中）
                        "y": 18,                          // 垂直偏移：18单位（光环纵向向上偏移18）
                        "haloRadius": 0,                  // 光环旋转半径：0（无额外旋转半径）
                        "haloRotateSpeed": -5,           // 旋转速度：-2.5（负号表示逆时针旋转）
                        "triLength": 20,                   // 三角形初始长度：30单位
                        "triLengthTo": 30,                 // 充能后长度：15单位（充能时三角形缩短到15）
                        "shapeRotation": 180,               // 形状旋转角度：180度（三角形自身翻转180度）
                        "color": "F2E87877",         // 光环颜色：浅蓝色（十六进制色码，含透明度）
                        "layer": 110,                      // 渲染层级：110（确保在塔体前景显示，不被遮挡）
                        "progress": "warmup"               // 效果触发条件：随充能进度变化（充能时才显示该特效）
                    }
                ]
            }
        ]
    }
